{
  "env": {
    "development": {
      "presets": ["es2015", "stage-2", "react"],
      "plugins": [["import-rename", { "^(.*)\\.scss$": "$1.css" }]]
    },
    "production": {
      "presets": [["es2015", { "modules": false }], "stage-2", "react"],
      "plugins": [
        ["external-helpers", ["import-rename", { "^(.*)\\.scss$": "$1.css" }]]
      ]
    }
  }
}
