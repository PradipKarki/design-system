{
  "name": "@cwds/components",
  "version": "0.2.0-alpha.11",
  "description": "React components for CWDS",
  "main": "lib/index.js",
  "module": "lib/index.esm.js",
  "style": "lib/index.css",
  "author": "Danny Eck <danny.eck@osi.ca.gov>",
  "license": "AGPL-3.0",
  "private": false,
  "scripts": {
    "start": "yarn storybook -p 3002",
    "//build": "NODE_ENV=production rollup -c",
    "//build:watch": "NODE_ENV=production rollup -c -w",
    "build": "yarn clean && yarn build:es && yarn build:esm && yarn build:css",
    "build:esm": "NODE_ENV=production BABEL_ENV=modules babel ./src/index.js --out-file ./lib/index.esm.js",
    "build:es": "NODE_ENV=production babel ./src --out-dir ./lib --ignore *.test.js,*.stories.js",
    "build:css": "NODE_ENV=production gulp dumbcss",
    "build:best": "gulp build:best",
    "build:watch": "gulp build:watch",
    "//prepublishOnly": "TODO: clean up the package.json file",
    "//prepare": "yarn run build",
    "clean:lib": "mkdir -p lib && rimraf lib*",
    "clean:dist": "mkdir -p dist && rimraf dist*",
    "clean": "yarn clean:lib && yarn clean:dist",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "test": "NODE_ENV=development jest --env=jsdom"
  },
  "dependencies": {
    "@fortawesome/fontawesome": "^1.1.5",
    "@fortawesome/fontawesome-free-solid": "^5.0.10",
    "@fortawesome/react-fontawesome": "^0.0.18",
    "bootstrap": "^4.1.0",
    "classnames": "^2.2.5",
    "hoist-non-react-statics": "^2.5.5",
    "lodash.omit": "^4.5.0",
    "lodash.pick": "^4.4.0",
    "prop-types": "^15.6.1",
    "react": "^16.3.2",
    "react-display-name": "^0.2.4",
    "react-dom": "^16.3.2",
    "react-overlays": "^0.8.3",
    "react-select": "^1.2.1",
    "react-sticky": "^6.0.2",
    "reactstrap": "^6.2.0"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^3.4.1",
    "@storybook/addon-info": "^3.4.1",
    "@storybook/addon-knobs": "^3.4.1",
    "@storybook/addon-links": "^3.4.1",
    "@storybook/addon-notes": "^3.4.1",
    "@storybook/addon-options": "^3.4.3",
    "@storybook/addons": "^3.4.1",
    "@storybook/react": "^3.4.1",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-jest": "^22.4.3",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-import-rename": "^1.0.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "gulp": "^3.9.1",
    "gulp-babel": "^7.0.1",
    "gulp-postcss": "^7.0.1",
    "gulp-rename": "^1.2.2",
    "gulp-sass": "^4.0.1",
    "husky": "^0.14.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^22.4.3",
    "jest-environment-jsdom": "^22.4.3",
    "lint-staged": "^7.0.4",
    "mkdirp": "^0.5.1",
    "node-sass-tilde-importer": "^1.0.2",
    "prettier": "^1.12.0",
    "rollup": "^0.58.0",
    "rollup-plugin-babel": "^3.0.3",
    "rollup-plugin-commonjs": "^9.1.0",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-peer-deps-external": "^2.1.0",
    "rollup-plugin-postcss": "^1.6.1",
    "rollup-plugin-uglify": "^3.0.0",
    "rollup-plugin-url": "^1.4.0"
  },
  "peerDependencies": {
    "classnames": "^2.2.5",
    "prop-types": "^15.6.1",
    "react": "^16.3.1",
    "react-dom": "^16.3.1"
  }
}
